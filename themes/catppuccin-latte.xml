<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="catppuccin-latte" _name="Catppuccin Latte" version="1.0">
  <author>Catppuccin Team</author>
  <_description>Soothing pastel theme for GNOME Text Editor</_description>

  <style name="text" foreground="#4c4f69" background="#eff1f5"/>
  <style name="selection" background="#ccd0da"/>
  <style name="cursor" foreground="#d20f39"/>

  <style name="def:comment" foreground="#9ca0b0" italic="true"/>
  <style name="def:keyword" foreground="#d20f39" bold="true"/>
  <style name="def:operator" foreground="#1e66f5"/>
  <style name="def:constant" foreground="#fe640b"/>
  <style name="def:string" foreground="#40a02b"/>
  <style name="def:number" foreground="#df8e1d"/>
  <style name="def:function" foreground="#8839ef"/>
  <style name="def:identifier" foreground="#ea76cb"/>
  <style name="def:type" foreground="#1e66f5"/>
  <style name="def:preprocessor" foreground="#179299"/>
  <style name="def:error" foreground="#d20f39" underline="true"/>
  <style name="def:warning" foreground="#fe640b" underline="true"/>
</style-scheme>
